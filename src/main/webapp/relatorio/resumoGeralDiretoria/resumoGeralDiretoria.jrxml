         COALESCE(( SELECT SUM(PC2.MASCULINO+PC2.FEMININO)
                  FROM PROFESSORCIVIL PC2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = PC2.ORGANIZACAO_ID
