        COALESCE(SUM(MAGISTERIOSUPERIOR_DESMIL_GRADUACAO),0) AS MAGISTERIOSUPERIOR_GRADUACAO



 FROM (


   SELECT  DISTINCT SS.SUPERIOR_ID as SUPERIOR_ID,
