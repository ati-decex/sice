<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ins_mon_exercito" language="groovy" pageWidth="208" pageHeight="15" columnWidth="208" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="id_om" class="java.lang.Integer"/>
	<parameter name="id_periodo" class="java.lang.Integer"/>
	<parameter name="id_om_enquadrante" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	sum(ofi.masculino) AS ofi_mas,
	sum(ofi.feminino) AS ofi_fem,
	sum(pra.masculino) AS pra_mas,
	sum(pra.feminino) AS pra_fem,
	sum((pra.masculino+pra.feminino+ofi.masculino+ofi.feminino)) AS total
FROM alunosmilitaroutrasforcas pra

INNER JOIN combotela ct on ct.organizacao_usuario_id = 146 and ct.tela_id =6
INNER JOIN alunosmilitaroutrasforcas ofi on
ofi.organizacao_id = ct.organizacao_vinculada_para_tela_id and ofi.periodo_id = $P{id_periodo}
AND ofi.tipoalunosmilitaroutrasforcas='OFICIAL'

WHERE
pra.organizacao_id = ct.organizacao_vinculada_para_tela_id and pra.periodo_id = $P{id_periodo}
AND pra.tipoalunosmilitaroutrasforcas='PRACA'
]]>
	</queryString>
	<field name="ofi_mas" class="java.lang.String"/>
	<field name="ofi_fem" class="java.lang.String"/>
	<field name="pra_mas" class="java.lang.String"/>
	<field name="pra_fem" class="java.lang.String"/>
	<field name="total" class="java.lang.String"/>
