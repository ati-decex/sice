	<parameter name="id_diretoria" class="java.lang.Long"/>
	<parameter name="id_periodo" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT DISTINCT a.data,TO_CHAR(a.data, 'DD/MM/YYYY') as data_formatada,u.postograduacao,u.nomeguerra,om.sigla,u.tel_funcional,u.email,u.nome,u.identidade
FROM auditoria a
INNER JOIN periodo p on p.id = $P{id_periodo}
INNER JOIN organizacaomilitar om on om.id = a.organizacaoid
INNER JOIN usuario u on u.id = a.usuarioid
INNER JOIN superiores_subordinados ss on ss.superior_subordinado_id = om.id
WHERE a.data between p.datainicial and p.datafinal and
ss.superior_id = $P{id_diretoria}
ORDER BY a.data DESC ,om.sigla ASC;]]>
	</queryString>
