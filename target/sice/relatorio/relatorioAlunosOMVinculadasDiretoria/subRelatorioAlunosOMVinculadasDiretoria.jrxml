	<queryString>
		<![CDATA[SELECT
	DISTINCT om.id , om.sigla
FROM organizacaomilitar om

INNER JOIN superiores_subordinados ss ON ss.superior_id = om.id and om.id != 2
INNER JOIN organizacaomilitar omv ON (ss.superior_subordinado_id = omv.id  AND omv.TIPO = 'VINCULADA')

WHERE om.id = $P{id_om}]]>
	</queryString>
