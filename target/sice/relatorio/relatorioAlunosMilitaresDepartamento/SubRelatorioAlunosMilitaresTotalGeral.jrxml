<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="report1" language="groovy" pageWidth="469" pageHeight="15" columnWidth="469" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="id_periodo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT 1 AS INDICE,
		'EXERCITO' AS EXERCITO,
        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_OFICIAL_FEMININO),0) AS ALUNO_EXERCITO_DESMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_OFICIAL_MASCULINO),0) AS ALUNO_EXERCITO_DESMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_PRACA_FEMININO),0) AS ALUNO_EXERCITO_DESMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_PRACA_MASCULINO),0) AS ALUNO_EXERCITO_DESMIL_PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_EXERCITO_DETMIL_OFICIAL_FEMININO),0) AS ALUNO_EXERCITO_DETMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_DETMIL_OFICIAL_MASCULINO),0) AS ALUNO_EXERCITO_DETMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_EXERCITO_DETMIL_PRACA_FEMININO),0) AS ALUNO_EXERCITO_DETMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_DETMIL_PRACA_MASCULINO),0) AS ALUNO_EXERCITO_DETMIL_PRACA_MASCULINO,



        COALESCE(SUM(ALUNO_EXERCITO_CCFEX_OFICIAL_FEMININO),0) AS ALUNO_EXERCITO_CCFEX_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_CCFEX_OFICIAL_MASCULINO),0) AS ALUNO_EXERCITO_CCFEX_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_EXERCITO_CCFEX_PRACA_FEMININO),0) AS ALUNO_EXERCITO_CCFEX_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_EXERCITO_CCFEX_PRACA_MASCULINO),0) AS ALUNO_EXERCITO_CCFEX_PRACA_MASCULINO,



        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_OFICIAL_MASCULINO) + SUM(ALUNO_EXERCITO_DETMIL_OFICIAL_MASCULINO)
       + SUM(ALUNO_EXERCITO_CCFEX_OFICIAL_MASCULINO),0) AS OFICIAIS_MASCULINO,

        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_OFICIAL_FEMININO) + SUM(ALUNO_EXERCITO_DETMIL_OFICIAL_FEMININO)
       + SUM(ALUNO_EXERCITO_CCFEX_OFICIAL_FEMININO),0) AS OFICIAIS_FEMININO,

        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_PRACA_MASCULINO) + SUM(ALUNO_EXERCITO_DETMIL_PRACA_MASCULINO)
        + SUM(ALUNO_EXERCITO_CCFEX_PRACA_MASCULINO),0) AS PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_EXERCITO_DESMIL_PRACA_FEMININO) + SUM(ALUNO_EXERCITO_DETMIL_PRACA_FEMININO)
       + SUM(ALUNO_EXERCITO_CCFEX_PRACA_FEMININO),0) AS PRACA_FEMININO

 FROM (

 SELECT   DISTINCT
       COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DESMIL_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DESMIL_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DESMIL_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DESMIL_PRACA_MASCULINO ,

         COALESCE(( SELECT SUM(AMOF2.FEMININO)
                   FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DETMIL_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DETMIL_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DETMIL_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_DETMIL_PRACA_MASCULINO ,




         COALESCE(( SELECT SUM(AMOF2.FEMININO)
                   FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_CCFEX_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'OFICIAL'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_CCFEX_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_CCFEX_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM MILITARESOMDS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITARESOMDS = 'PRACA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_EXERCITO_CCFEX_PRACA_MASCULINO


        FROM MILITARESOMDS AMOF
        INNER JOIN  SUPERIORES_SUBORDINADOS SS ON SS.SUPERIOR_SUBORDINADO_ID = AMOF.ORGANIZACAO_ID

        WHERE  AMOF.PERIODO_ID = $P{id_periodo}


) TABELA

UNION

SELECT DISTINCT 2 AS INDICE,
'MARINHA' AS MARINHA,
        COALESCE(SUM(ALUNO_MARINHA_DESMIL_OFICIAL_FEMININO),0) AS ALUNO_MARINHA_DESMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_DESMIL_OFICIAL_MASCULINO),0) AS ALUNO_MARINHA_DESMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_MARINHA_DESMIL_PRACA_FEMININO),0) AS ALUNO_MARINHA_DESMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_DESMIL_PRACA_MASCULINO),0) AS ALUNO_MARINHA_DESMIL_PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_MARINHA_DETMIL_OFICIAL_FEMININO),0) AS ALUNO_MARINHA_DETMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_DETMIL_OFICIAL_MASCULINO),0) AS ALUNO_MARINHA_DETMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_MARINHA_DETMIL_PRACA_FEMININO),0) AS ALUNO_MARINHA_DETMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_DETMIL_PRACA_MASCULINO),0) AS ALUNO_MARINHA_DETMIL_PRACA_MASCULINO,



        COALESCE(SUM(ALUNO_MARINHA_CCFEX_OFICIAL_FEMININO),0) AS ALUNO_MARINHA_CCFEX_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_CCFEX_OFICIAL_MASCULINO),0) AS ALUNO_MARINHA_CCFEX_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_MARINHA_CCFEX_PRACA_FEMININO),0) AS ALUNO_MARINHA_CCFEX_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_MARINHA_CCFEX_PRACA_MASCULINO),0) AS ALUNO_MARINHA_CCFEX_PRACA_MASCULINO,


        COALESCE(SUM(ALUNO_MARINHA_DESMIL_OFICIAL_MASCULINO) + SUM(ALUNO_MARINHA_DETMIL_OFICIAL_MASCULINO)
       + SUM(ALUNO_MARINHA_CCFEX_OFICIAL_MASCULINO),0) AS OFICIAIS_MASCULINO,

        COALESCE(SUM(ALUNO_MARINHA_DESMIL_OFICIAL_FEMININO) + SUM(ALUNO_MARINHA_DETMIL_OFICIAL_FEMININO)
        + SUM(ALUNO_MARINHA_CCFEX_OFICIAL_FEMININO),0) AS OFICIAIS_FEMININO,

        COALESCE(SUM(ALUNO_MARINHA_DESMIL_PRACA_MASCULINO) + SUM(ALUNO_MARINHA_DETMIL_PRACA_MASCULINO)
        + SUM(ALUNO_MARINHA_CCFEX_PRACA_MASCULINO),0) AS PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_MARINHA_DESMIL_PRACA_FEMININO) + SUM(ALUNO_MARINHA_DETMIL_PRACA_FEMININO)
       + SUM(ALUNO_MARINHA_CCFEX_PRACA_FEMININO),0) AS PRACA_FEMININO


 FROM (



 SELECT   DISTINCT
       COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DESMIL_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DESMIL_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DESMIL_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 2
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DESMIL_PRACA_MASCULINO ,

         COALESCE(( SELECT SUM(AMOF2.FEMININO)
                   FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DETMIL_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DETMIL_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DETMIL_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID IN(4,146)
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_DETMIL_PRACA_MASCULINO ,




         COALESCE(( SELECT SUM(AMOF2.FEMININO)
                   FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_CCFEX_OFICIAL_FEMININO ,

        COALESCE(( SELECT SUM(AMOF2.MASCULINO) AS TOTAL
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'OFICIAL'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_CCFEX_OFICIAL_MASCULINO  ,

        COALESCE(( SELECT SUM(AMOF2.FEMININO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_CCFEX_PRACA_FEMININO,

       COALESCE(( SELECT SUM(AMOF2.MASCULINO)
                  FROM ALUNOSMILITAROUTRASFORCAS AMOF2
                  INNER JOIN  SUPERIORES_SUBORDINADOS SS2 ON SS2.SUPERIOR_SUBORDINADO_ID = AMOF2.ORGANIZACAO_ID
                  AND AMOF2.TIPOALUNOSMILITAROUTRASFORCAS = 'PRACA'
                  AND AMOF2.FORCA = 'MARINHA'
                  AND SS2.SUPERIOR_ID = 6
                  AND AMOF2.PERIODO_ID = AMOF.PERIODO_ID

        ),0) AS ALUNO_MARINHA_CCFEX_PRACA_MASCULINO


        FROM ALUNOSMILITAROUTRASFORCAS AMOF
        INNER JOIN  SUPERIORES_SUBORDINADOS SS ON SS.SUPERIOR_SUBORDINADO_ID = AMOF.ORGANIZACAO_ID

        WHERE  AMOF.PERIODO_ID = $P{id_periodo}


) TABELA


UNION




 SELECT DISTINCT 3 AS INDICE,
 'FORÇA AÉREA' AS FORCAAEREA,
        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_OFICIAL_FEMININO),0) AS ALUNO_FORCAAEREA_DESMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_OFICIAL_MASCULINO),0) AS ALUNO_FORCAAEREA_DESMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_PRACA_FEMININO),0) AS ALUNO_FORCAAEREA_DESMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_PRACA_MASCULINO),0) AS ALUNO_FORCAAEREA_DESMIL_PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_FORCAAEREA_DETMIL_OFICIAL_FEMININO),0) AS ALUNO_FORCAAEREA_DETMIL_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DETMIL_OFICIAL_MASCULINO),0) AS ALUNO_FORCAAEREA_DETMIL_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DETMIL_PRACA_FEMININO),0) AS ALUNO_FORCAAEREA_DETMIL_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_DETMIL_PRACA_MASCULINO),0) AS ALUNO_FORCAAEREA_DETMIL_PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_FORCAAEREA_CCFEX_OFICIAL_FEMININO),0) AS ALUNO_FORCAAEREA_CCFEX_OFICIAL_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_CCFEX_OFICIAL_MASCULINO),0) AS ALUNO_FORCAAEREA_CCFEX_OFICIAL_MASCULINO,
        COALESCE(SUM(ALUNO_FORCAAEREA_CCFEX_PRACA_FEMININO),0) AS ALUNO_FORCAAEREA_CCFEX_PRACA_FEMININO,
        COALESCE(SUM(ALUNO_FORCAAEREA_CCFEX_PRACA_MASCULINO),0) AS ALUNO_FORCAAEREA_CCFEX_PRACA_MASCULINO,


          COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_OFICIAL_MASCULINO) + SUM(ALUNO_FORCAAEREA_DETMIL_OFICIAL_MASCULINO)
        + SUM(ALUNO_FORCAAEREA_CCFEX_OFICIAL_MASCULINO),0) AS OFICIAIS_MASCULINO,

        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_OFICIAL_FEMININO) + SUM(ALUNO_FORCAAEREA_DETMIL_OFICIAL_FEMININO)
        + SUM(ALUNO_FORCAAEREA_CCFEX_OFICIAL_FEMININO),0) AS OFICIAIS_FEMININO,

        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_PRACA_MASCULINO) + SUM(ALUNO_FORCAAEREA_DETMIL_PRACA_MASCULINO)
        + SUM(ALUNO_FORCAAEREA_CCFEX_PRACA_MASCULINO),0) AS PRACA_MASCULINO,

        COALESCE(SUM(ALUNO_FORCAAEREA_DESMIL_PRACA_FEMININO) + SUM(ALUNO_FORCAAEREA_DETMIL_PRACA_FEMININO)
        + SUM(ALUNO_FORCAAEREA_CCFEX_PRACA_FEMININO),0) AS PRACA_FEMININO




 FROM (



