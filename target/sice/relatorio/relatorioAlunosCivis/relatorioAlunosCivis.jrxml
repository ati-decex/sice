	<queryString>
		<![CDATA[SELECT DISTINCT
	COALESCE(p.masculino, 0) as pos_masc,
	COALESCE(p.feminino, 0) as pos_fem,
	COALESCE(m.masculino, 0) as mestrado_masc,
	COALESCE(m.feminino, 0) as mestrado_fem,
	COALESCE(d.masculino, 0) as doutorado_masc,
	COALESCE(d.feminino, 0) as doutorado_fem,
	COALESCE(pd.masculino, 0) as posdoutorado_masc,
	COALESCE(pd.feminino, 0) as posdoutorado_fem,

	om.sigla as sigla

FROM organizacaomilitar om

LEFT JOIN alunoscivis m ON
m.organizacao_id = $P{id_om} AND m.periodo_id = $P{id_periodo}
AND m.tipoescolaridade='MESTRE'

LEFT JOIN alunoscivis d ON
d.organizacao_id = $P{id_om} AND d.periodo_id = $P{id_periodo}
AND d.tipoescolaridade='DOUTORADO'

LEFT JOIN alunoscivis pd ON
pd.organizacao_id = $P{id_om} AND pd.periodo_id = $P{id_periodo}
AND pd.tipoescolaridade='POSDOUTORADO'

LEFT JOIN alunoscivis p ON
p.organizacao_id = $P{id_om} AND p.periodo_id = $P{id_periodo}
AND p.tipoescolaridade='POSGRADUACAO'

WHERE om.id = $P{id_om}]]>
	</queryString>
