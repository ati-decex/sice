    ),0) AS TOTAL_MASCULINO,

     COALESCE((SELECT SUM(PC1.FEMININO) AS TOTAL_FEMININO
    FROM PROFESSORCIVIL PC1
    INNER JOIN SUPERIORES_SUBORDINADOS SS2
      ON SS2.SUPERIOR_SUBORDINADO_ID = PC1.ORGANIZACAO_ID
