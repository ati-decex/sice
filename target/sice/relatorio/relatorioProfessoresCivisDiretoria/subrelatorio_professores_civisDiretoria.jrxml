    INNER JOIN SUPERIORES_SUBORDINADOS SS2
      ON SS2.SUPERIOR_SUBORDINADO_ID = PC1.ORGANIZACAO_ID
    WHERE PC1.REGIMEDETRABALHO = 'VINTEH'
      AND PC1.CARREIRA = PC.CARREIRA
      AND PC1.PERIODO_ID = PC.PERIODO_ID
