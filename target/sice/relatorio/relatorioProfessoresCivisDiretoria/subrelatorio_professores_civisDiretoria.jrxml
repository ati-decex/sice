FROM PROFESSORCIVIL PC
INNER JOIN SUPERIORES_SUBORDINADOS SS ON SS.SUPERIOR_SUBORDINADO_ID = PC.ORGANIZACAO_ID
INNER JOIN ORGANIZACAOMILITAR OM ON OM.ID = SS.SUPERIOR_SUBORDINADO_ID

