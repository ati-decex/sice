        COALESCE(SUM(MARINHA_INSTRUTOR_FEMININO) + SUM(MARINHA_MONITOR_FEMININO),0) AS TOTAL_FEMININO,

		COALESCE(SUM(MARINHA_INSTRUTOR_MASCULINO) + SUM(MARINHA_MONITOR_MASCULINO)
		+ SUM(MARINHA_INSTRUTOR_FEMININO) + SUM(MARINHA_MONITOR_FEMININO),0) AS TOTAL_EX,

