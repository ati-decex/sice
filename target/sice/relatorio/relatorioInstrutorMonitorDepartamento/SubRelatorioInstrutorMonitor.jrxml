                  WHERE IM2.TIPOINSTRUTOREMONITOR = 'MONITOR'
                  AND IM2.FORCAINSTRUTORES = 'FORCAAUXILIAR'
                  AND IM2.PERIODO_ID = IM.PERIODO_ID

        ),0) AS FORCAAUXILIAR_MONITOR_FEMININO,

       COALESCE(( SELECT SUM(IM2.MASCULINO)
                  FROM INSTRUTORMONITOR IM2
                  WHERE IM2.TIPOINSTRUTOREMONITOR = 'MONITOR'
                  AND IM2.FORCAINSTRUTORES = 'FORCAAUXILIAR'
                  AND IM2.PERIODO_ID = IM.PERIODO_ID

        ),0) AS FORCAAUXILIAR_MONITOR_MASCULINO

        FROM INSTRUTORMONITOR IM
        WHERE  IM.FORCAINSTRUTORES = 'FORCAAUXILIAR'
        AND IM.PERIODO_ID = $P{id_periodo}
) TABELA



UNION



 SELECT 5 AS INDICE,
        'BOMBEIROS' AS EXERCITO,
        COALESCE(SUM(MONITOR_DESMIL),0) AS MONITOR_DESMIL,
        COALESCE(SUM(INSTRUTOR_DESMIL),0) AS INSTRUTOR_DESMIL,
        COALESCE(SUM(INSTRUTOR_DETMIL),0) AS INSTRUTOR_DETMIL,
        COALESCE(SUM(MONITOR_DETMIL),0) AS MONITOR_DETMIL,
        COALESCE(SUM(INSTRUTOR_DEPA),0) AS INSTRUTOR_DEPA,
        COALESCE(SUM(MONITOR_DEPA),0) AS MONITOR_DEPA,
        COALESCE(SUM(INSTRUTOR_CCFEX),0) AS INSTRUTOR_CCFEX,
        COALESCE(SUM(MONITOR_CCFEX),0) AS MONITOR_CCFEX,
        COALESCE(SUM(BOMBEIRO_INSTRUTOR_FEMININO),0) AS EXERCITO_INSTRUTOR_FEMININO,
        COALESCE(SUM(BOMBEIRO_INSTRUTOR_MASCULINO),0) AS EXERCITO_INSTRUTOR_MASCULINO,
        COALESCE(SUM(BOMBEIRO_MONITOR_FEMININO),0) AS EXERCITO_MONITOR_FEMININO,
        COALESCE(SUM(BOMBEIRO_MONITOR_MASCULINO),0) AS EXERCITO_MONITOR_MASCULINO,

        COALESCE(SUM(INSTRUTOR_DESMIL) + SUM(MONITOR_DESMIL),0) AS TOTAL_DESMIL,
        COALESCE(SUM(INSTRUTOR_DETMIL) + SUM(MONITOR_DETMIL),0) AS TOTAL_DETMIL,
        COALESCE(SUM(INSTRUTOR_DEPA)  + SUM(MONITOR_DEPA),0) AS TOTAL_DEPA,
        COALESCE(SUM(INSTRUTOR_CCFEX) + SUM(MONITOR_CCFEX),0) AS TOTAL_CCFEX,
        COALESCE(SUM(BOMBEIRO_INSTRUTOR_MASCULINO) + SUM(BOMBEIRO_MONITOR_MASCULINO),0) AS TOTAL_MASCULINO,
        COALESCE(SUM(BOMBEIRO_INSTRUTOR_FEMININO) + SUM(BOMBEIRO_MONITOR_FEMININO),0) AS TOTAL_FEMININO,

		COALESCE(SUM(BOMBEIRO_INSTRUTOR_FEMININO) + SUM(BOMBEIRO_INSTRUTOR_MASCULINO)
		+ SUM(BOMBEIRO_MONITOR_FEMININO) + SUM(BOMBEIRO_MONITOR_MASCULINO),0) AS TOTAL_EX,

        COALESCE(SUM(BOMBEIRO_INSTRUTOR_MASCULINO) + SUM(BOMBEIRO_INSTRUTOR_FEMININO),0) AS TOTAL_INSTRUTOR,
        COALESCE(SUM(BOMBEIRO_MONITOR_MASCULINO) + SUM(BOMBEIRO_MONITOR_FEMININO),0) AS TOTAL_MONITOR


