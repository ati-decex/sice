        'DPHCEX' AS DPHCEX,
        COALESCE(SUM(EBTT_DPHCEX_DOUTOR),0) AS EBTT_DPHCEX_DOUTOR,
        COALESCE(SUM(MAGISTERIOSUPERIOR_DPHCEX_DOUTOR),0) AS MAGISTERIOSUPERIOR_DPHCEX_DOUTOR,

        COALESCE(SUM(EBTT_DPHCEX_MESTRE),0) AS PROFESOR_PERMANENTE_DPHCEX_MASCULINO,
        COALESCE(SUM(MAGISTERIOSUPERIOR_DPHCEX_MESTRE),0) AS MAGISTERIOSUPERIOR_DPHCEX_MESTRE,

